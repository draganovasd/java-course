import java.util.Scanner;
import java.util.Random;
public class TestMetods {

	public static void main(String[] args) {
		print(10);

		}
		
		public static void print(int number) {
		    if (number > 0) {
		    	number--;
		    	
		    	System.out.println(number);
		    	print(number);
		    } else {
			      return;
		    }
		  }

}
