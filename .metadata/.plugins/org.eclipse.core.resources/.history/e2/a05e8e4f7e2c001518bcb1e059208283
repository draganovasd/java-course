		Scanner input = new Scanner(System.in, "UTF-8");
		System.out.println("Please enter the three coefficients! ");
		System.out.print("a: ");
		double a=input.nextDouble();
		System.out.print("b: ");
		double b=input.nextDouble();
		System.out.print("c: ");
		double c=input.nextDouble();
		input.close();
	}
  
}
