public class From30To1 {

	public static void main(String[] args) {
		
		
			int maxRow=5,maxCol=6;
			int[][] elArr = new int[5][6];
			
			for(int i=0;i<maxRow;i++){
				for(int j=0;j<maxCol;j++){
					elArr[i][j]=1+j+6*i;
				}
			}
			
			for(int row = 0; row < elArr[maxCol].length; row++){
				for(int col = 0; col < elArr[maxRow].length; col++){
					System.out.print(elArr[row][col] + " ");
				}
				System.out.println();
			}
			
		}

}
