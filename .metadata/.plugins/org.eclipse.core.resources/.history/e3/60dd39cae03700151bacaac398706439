package CelsiusToFahrenheit;

import java.util.Scanner;

public class CelsiusToFahrenheit {
	private double celsius;
	private double fahrenheit;
	
	public CelsiusToFahrenheit(double n){
		this.celsius=1;
		this.fahrenheit=33.8*celsius;
	}
	
	public void CelsiusToFahrenheit(double celsius){
		setCelsius(celsius);
		
	}
	public void setCelsius(double celsius){
		this.celsius=celsius;
	}
	public double getCelsius(){
		return celsius;
	}
	
	
	public double getFahrenheit(){
		return this.fahrenheit;
	}
	public void printResult(){
		System.out.println(this.celsius+"to fahrenheit is : "+this.fahrenheit);
	}
	
	public static void main(String[] args) {
		Scanner input = new Scanner(System.in, "UTF-8");
		double n=input.nextDouble();
		CelsiusToFahrenheit temp=new CelsiusToFahrenheit(n);
		temp.printResult();

	}

}
