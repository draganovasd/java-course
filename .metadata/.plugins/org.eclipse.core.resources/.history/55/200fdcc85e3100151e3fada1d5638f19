public class From30To1 {

	public static void main(String[] args) {
		
		int Array[][] = new int[5][6];
		for(int i=30; i>0 ; i-- ){
			for(int j=30; j>0; j--)
			Array[i][j]=i;
		}

		for(int row = 0; row < Array.length; row++){
			for(int col = 0; col < Array[row].length; col++){
				System.out.print(Array[row][col] + " ");
			}
			System.out.println();
		}
		

	}

}
