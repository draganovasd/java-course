public class From30To1 {

	public static void main(String[] args) {
		
		int Array[][] = new int[5][6];
		int arr;
		int i=0, j=0 ,x=0;

		for( i=0; i < Array.length ; i++ ){
			for( j=0 ; j<Array.length ; j++){
			for( x=30; x>1 ; x--){
				Array[i][j]=x;
			}
			System.out.print(Array[i][j] + " ");
			}
			System.out.println();
		}
	
	}

}
