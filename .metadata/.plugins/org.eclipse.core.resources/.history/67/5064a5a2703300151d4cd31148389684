public class From30To1 {

	public static void main(String[] args) {
		int row=5 ; 
		int col=6;
		int Array[][] = new int[row][col];
		int arr;
		int i=0, j=0 ,x=0;

		for( i=0; i < Array[col].length ; i++ ){
			for( j=0 ; j<Array[row].length ; j++){
			for( x=30; x>1 ; x--){
				Array[i][j]=1+j+6*i;
			}
			System.out.print(Array[i][j] + " ");
			}
			System.out.println();
		}
	
	}

}
